<html>
<head>
<title>CustomerPortalSite</title>
</head>

<body>
<table border="1" cellpadding="1" cellspacing="1">
<thead>
<tr><td rowspan="1" colspan="3">CustomerPortalSite</td></tr>
</thead>

<tbody>
<tr>
	<td>ACCOUNTS_TABLE</td>
	<td>//tr[(@data-qa-id='row') and contains(., '${value}') and contains(., 'Active')]</td>
	<td></td>
</tr>
<tr>
	<td>OBJECTS_TABLE</td>
	<td>//div[(@class='dnd-tbody') and contains(., '${value}') and contains(., 'Approved')]</td>
	<td></td>
</tr>
<tr>
	<td>CERTIFICATE</td>
	<td>//button[contains(@onclick, 'replace') and contains(@onclick, '${value}') and contains(., 'Replace')]</td>
	<td></td>
</tr>
<tr>
	<td>OKTA_USERNAME</td>
	<td>//input[contains(@id, 'okta-signin-username')]</td>
	<td></td>
</tr>
<tr>
	<td>OKTA_PASSWORD</td>
	<td>//input[contains(@id, 'okta-signin-password')]</td>
	<td></td>
</tr>
<tr>
	<td>OKTA_SUBMIT</td>
	<td>//input[contains(@id, 'okta-signin-submit')]</td>
	<td></td>
</tr>
<tr>
	<td>CARD_NAME</td>
	<td>//div[contains(@class, 'cards-subscription')]//h5[(text()='${cardName}')]</td>
	<td></td>
</tr>
<tr>
	<td>CARD_STATUS</td>
	<td>//div[contains(@class, 'card-subscription')]//h5[(text()='${cardName}')]//..//span[(text()='${status}')]</td>
	<td></td>
</tr>
<tr>
	<td>SUBSCRIPTION_NAVBAR</td>
	<td>//div[@id='subscription-navbar']/button[@value='${value}']</td>
	<td></td>
</tr>
<tr>
	<td>TABLE_HEAD</td>
	<td>//thead//th[text()= '${title}']</td>
	<td></td>
</tr>
<tr>
	<td>TABLE_VALUE</td>
	<td>//tbody//td[text()= '${value}']</td>
	<td></td>
</tr>
<tr>
	<td>TABLE_STATUS</td>
	<td>//td[contains(@class, 'text-center')]/span[contains(@class, 'label')]/span[(text()= '${status}')]</td>
	<td></td>
</tr>
<tr>
	<td>MODAL</td>
	<td>//div[contains(@class, 'modal-dialog')]</td>
	<td></td>
</tr>
<tr>
	<td>MODAL_TITLE</td>
	<td>//div[contains(@class, 'flex-row')]//h2[contains(., '${modalTitle}')]</td>
	<td></td>
</tr>
<tr>
	<td>CLOSE_MODAL_CARD</td>
	<td>//button[contains(@aria-label, 'close')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>